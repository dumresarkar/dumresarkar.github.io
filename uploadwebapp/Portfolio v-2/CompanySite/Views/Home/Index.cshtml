@model CompanySite.Models.Home

@{
    ViewBag.Title = "Top";
}

<!-- ======= home Section ======= -->
<section id="home" class="hero d-flex align-items-center">

    <div class="container">
        <div class="row">
            <div class="col-lg-6 d-flex flex-column justify-content-center">
                <h1 data-aos="fade-up">Grow your business collaborating with us</h1>
                <h2 data-aos="fade-up" data-aos-delay="400">We are team of talented developers making mobile apps and robust websites</h2>
                <div data-aos="fade-up" data-aos-delay="600">
                    <div class="text-center text-lg-start">
                        <a href="#about" class="btn-get-started scrollto d-inline-flex align-items-center justify-content-center align-self-center">
                            <span>Get Started</span>
                            <i class="bi bi-arrow-right"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 hero-img" data-aos="zoom-out" data-aos-delay="200">
                <img src="~/Content/img/hero-img.png" class="img-fluid" alt="">
            </div>
        </div>
    </div>

</section><!-- End Hero -->

<main id="main">
    <!-- ======= About Section ======= -->
    <section id="about" class="about">

        <div class="container" data-aos="fade-up">
            <div class="row gx-0">

                <div class="col-lg-6 d-flex flex-column justify-content-center" data-aos="fade-up" data-aos-delay="200">
                    <div class="content">
                        <h3>Who We Are</h3>
                        <h2>Innovate Efficiently Better, Faster, Bigger.</h2>
                        <p>
                            We are a mission-driven organization which fuel the mission with a team of passionate well trained and motivated individual.<br/>
                            We help businesses plan and create the digital experience of tomorrow. We succeed together by building the best of entrepreneurship, startup, rational and world-class engineering.
                        </p>
                        <h2>Design, Bulid and Scale your vision.</h2>
                        <p>
                            We will build you custom websites and manage them as well.<br />
                            We are effective and creative web design & development company in Kathmandu, Nepal. We build robust websites and software applications.
                        </p>
                        <h2>We help ambitious clients reach unheard heights with extraordinary outcomes.</h2>
                        <p>
                            Our company provides an extraordinary environment and opportunity to make right use of new technology. Great teamwork and amity with co-worker.
                        </p>
                    </div>
                </div>

                <div class="col-lg-6 d-flex align-items-center" data-aos="zoom-out" data-aos-delay="200">
                    <img src="~/Content/img/about.jpg" class="img-fluid" alt="">
                </div>

            </div>
        </div>

    </section><!-- End About Section -->
    <!-- ======= Services Section ======= -->
    <section id="services" class="services">

        <div class="container" data-aos="fade-up">

            <header class="section-header">
                <h2>Services</h2>
                <p>Service to grow your business</p>
            </header>

            <div class="row gy-4">

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="200">
                    <div class="service-box blue">
                        <i class="ri-discuss-line icon"></i>
                        <h3>Outsourcing</h3>
                        <p>Our strength is our capacity in bringing projects from different parts of the world without any communication or external barriers.</p>
                        @*<a href="#" class="read-more"><span>Read More</span> <i class="bi bi-arrow-right"></i></a>*@
                    </div>
                </div>

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="300">
                    <div class="service-box orange">
                        <i class="ri-discuss-line icon"></i>
                        <h3>Mobile Apps</h3>
                        <p>We help build robust mobile applications which are intended for productivity assistance as email, calandar and contact databases.</p>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="400">
                    <div class="service-box green">
                        <i class="ri-discuss-line icon"></i>
                        <h3>Responsive Websites</h3>
                        <p>Our approach is to design webpages which render well on a variety of devices and window or screen sizes.</p>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="500">
                    <div class="service-box red">
                        <i class="ri-discuss-line icon"></i>
                        <h3>E-commerce</h3>
                        <p>Our products helps in buying and ordering of goods and sercives through differnts systems like POS system.</p>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="600">
                    <div class="service-box purple">
                        <i class="ri-discuss-line icon"></i>
                        <h3>Enterprise Application</h3>
                        <p>An enterprise application (EA) is a large software system platform designed to operate in a corporate environment such as business or government.</p>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="700">
                    <div class="service-box pink">
                        <i class="ri-discuss-line icon"></i>
                        <h3>CMS</h3>
                        <p>A Content Management System, or CMS, is a piece of software designed to help users create and edit a website. We build such system.</p>
                    </div>
                </div>

            </div>

        </div>

    </section><!-- End Services Section -->

    <!-- ======= Products Section ======= -->
    <section id="products" class="values">

        <div class="container" data-aos="fade-up">

            <header class="section-header">
                <h2>Our Products</h2>
                <p>Our products are coming soon.........</p>
            </header>

            <div class="row">

                <div class="col-lg-6">
                    <div class="box" data-aos="fade-up" data-aos-delay="100" style="text-align: center;">
